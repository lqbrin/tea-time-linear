\begin_inset Formula $A\left[\begin{array}{c}
4\\
-1
\end{array}\right]=\left[\begin{array}{c}
-12\\
3
\end{array}\right]$
\end_inset

, 
\begin_inset Formula $A\left[\begin{array}{c}
-\frac{8}{3}\\
\frac{2}{3}
\end{array}\right]=\left[\begin{array}{c}
8\\
-2
\end{array}\right]$
\end_inset

, 
\begin_inset Formula $A\left[\begin{array}{c}
1\\
1
\end{array}\right]=\left[\begin{array}{c}
2\\
2
\end{array}\right]$
\end_inset

, and 
\begin_inset Formula $A\left[\begin{array}{c}
-\frac{1}{2}\\
-\frac{1}{2}
