\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
\begin{gathered}L\left((a_{1}x^{2}+b_{1}x+c_{1})+(a_{2}x^{2}+b_{2}x+c_{2})\right)\\
=L\left((a_{1}+a_{2})x^{2}+(b_{1}+b_{2})x+(c_{1}+c_{2})\right)\\
=6(a_{1}+a_{2})x+3(b_{1}+b_{2})\\
=6a_{1}x+6a_{2}x+3b_{1}+3b_{2}\\
=\left(6a_{1}x+3b_{1}\right)+\left(6a_{2}x+3b_{2}\right)\\
=L(a_{1}x^{2}+b_{1}x+c_{1})+L(a_{2}x^{2}+b_{2}x+c_{2})
\end{gathered}
\]

\end_inset


\end_layout

\begin_layout Enumerate
